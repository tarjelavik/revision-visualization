{
  "name": "revision-networks",
  "version": "1.0.0",
  "author": "Ahl Nilsen",
  "license": "ISC",
  "description": "The app for the network component of the Revision project",
  "main": "./dist/server/src/app.js",
  "scripts": {
    "install:server": "npm install",
    "install:client": "cd client && npm install",
    "installAll": "npm run install:server && npm run install:client",
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "start:dev:server": "nodemon",
    "start:dev:client": "cd client && npm run start",
    "start:server": "tsc && node dist/server/src/app.js",
    "pre-lint-msg": "Echo Linting project... && exit 0",
    "lint": "npx eslint . --ext .ts,.tsx",
    "pre-test-msg": "Echo running test suite... && exit 0",
    "test:server": "npx jest",
    "test:client": "cd client && npm run test && exit 0",
    "fix": "npx eslint . --ext .ts,.tsx --fix"
  },
  "pre-commit": [
    "pre-test-msg",
    "test:server",
    "test:client"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.app.uib.no:revision/revision-visualization.git"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/enzyme": "^3.10.8",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.17.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "nodemon": "^2.0.4",
    "pre-commit": "^1.2.2",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "uuid": "^8.3.1"
  }
}
