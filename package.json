{
  "name": "revision-visualization",
  "version": "1.0.0",
  "description": "This repo contains code and documentation for the visualization component of the Revision project",
  "main": "index.js",
  "scripts": {
    "install:server": "npm install",
    "install:client": "cd client && npm install",
    "installAll": "npm run install:server && npm run install:client",
    "start": "(tsc && node dist/server/src/app.js) & cd client && npm run start",
    "startClient": "cd client && npm run start",
    "conc": "concurrently \"npm run start\" \"npm run startClient \"",
    "lint": "tslint -p .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.app.uib.no:revision/revision-visualization.git"
  },
  "author": "Ahl Nilsen",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "pre-commit": "^1.2.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/jest": "^26.0.13",
    "concurrently": "^5.3.0",
    "express": "^4.17.1"
  }
}
